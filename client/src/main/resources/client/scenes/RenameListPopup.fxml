<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane maxHeight="1.7976931348623157E308" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.scenes.RenameListPopupCtrl">
    <bottom>
        <ButtonBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
            <buttons>
                <Button mnemonicParsing="false" onAction="#close" text="Cancel" />
                <Button mnemonicParsing="false" onAction="#save" text="Save" />
            </buttons>
            <BorderPane.margin>
                <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
            </BorderPane.margin>
        </ButtonBar>
    </bottom>
    <center>
        <ScrollPane fitToHeight="true" hbarPolicy="NEVER" hmax="0.0" maxHeight="1.7976931348623157E308" BorderPane.alignment="CENTER">
            <content>
                <VBox maxHeight="1.7976931348623157E308">
                    <children>
                        <TextField fx:id="listTitle" prefHeight="26.0" prefWidth="222.0">
                            <VBox.margin>
                                <Insets />
                            </VBox.margin>
                        </TextField>
                    </children>
                    <padding>
                        <Insets bottom="8.0" left="8.0" right="20.0" top="8.0" />
                    </padding>
                </VBox>
            </content>
        </ScrollPane>
    </center>
</BorderPane>
